; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Gene Quest"
run/main_scene="res://scenes/Menu.tscn"
config/features=PackedStringArray("4.3", "C#", "Mobile")
boot_splash/image="res://assets/BG/Splash.png"
config/icon="res://icon.png"

[autoload]

GameState="*res://scripts/GameState.cs"

[display]

window/size/viewport_width=1500
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Chromo Game"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
